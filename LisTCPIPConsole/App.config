<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="LisTCPIPConsole.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
  </configSections>
  <log4net debug="true">
    <appender name="RollingLogFileAppenderError" type="log4net.Appender.RollingFileAppender">
      <file value="log\exceptions.txt"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="10MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level - %message%newline"/>
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <!--
		  ALL
		  DEBUG
		  INFO
		  WARN
		  ERROR
		  FATAL
		  OFF
		  -->
        <levelMin value="WARN"/>
        <levelMax value="FATAL"/>
      </filter>
    </appender>
    <appender name="RollingLogFileAppenderDebug" type="log4net.Appender.RollingFileAppender">
      <file value="log\debug.txt"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="10MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level - %message%newline"/>
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <!--
		  ALL
		  DEBUG
		  INFO
		  WARN
		  ERROR
		  FATAL
		  OFF
		  -->
        <levelMin value="DEBUG"/>
        <levelMax value="FATAL"/>
      </filter>
    </appender>
    <appender name="RollingLogFileAppenderInfo" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="log\"/>
      <datePattern value="dd.MM.yyyy'_log.txt'"/>
      <staticLogFileName value="false"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level - %message%newline"/>
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <!--
		  ALL
		  DEBUG
		  INFO
		  WARN
		  ERROR
		  FATAL
		  OFF
		  -->
        <levelMin value="INFO"/>
        <levelMax value="INFO"/>
      </filter>
    </appender>
    <appender name="LogTextBoxAppender" type="LisTCPIPConsole.LogTextBoxAppender, LisTCPIPConsole">
      <formName value="Home"/>
      <textBoxName value="txtLog"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level - %message%newline"/>
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <!--
		  ALL
		  DEBUG
		  INFO
		  WARN
		  ERROR
		  FATAL
		  OFF
		  -->
        <levelMin value="INFO"/>
        <levelMax value="FATAL"/>
      </filter>
    </appender>
    <root>
      <level value="ALL"/>
      <appender-ref ref="RollingLogFileAppenderDebug"/>
      <appender-ref ref="RollingLogFileAppenderInfo"/>
      <appender-ref ref="RollingLogFileAppenderError"/>
      <appender-ref ref="LogTextBoxAppender"/>
    </root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
  <userSettings>
    <LisTCPIPConsole.Properties.Settings>
      <setting name="AUTO_CONNECT" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="RUN_ON_STARTUP" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="SERVER_URL" serializeAs="String">
        <value>https://localhost:44392</value>
      </setting>
      <setting name="API_KEY" serializeAs="String">
        <value>DUMMY</value>
      </setting>
      <setting name="IP_ADDRESS" serializeAs="String">
        <value>10.228.175.184</value>
      </setting>
      <setting name="PORT_NO" serializeAs="String">
        <value>1234</value>
      </setting>
      <setting name="EQUIPMENT_TYPE" serializeAs="String">
        <value>BS430</value>
      </setting>
    </LisTCPIPConsole.Properties.Settings>
  </userSettings>
</configuration>
